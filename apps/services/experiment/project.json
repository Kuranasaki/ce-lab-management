{
  "name": "experiment_svc",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/experiment/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx-bun/nx:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "entrypoints": ["apps/services/experiment/src/index.ts"],
        "outputPath": "dist/experiment-svc/src/index.ts",
        "bun": false,
        "smol": false
      }
    },
    "serve": {
      "executor": "@nx-bun/nx:run",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "experiment_svc:build",
        "watch": true,
        "hot": true,
        "bun": true,
        "smol": false
      }
    }
  },
  "tags": []
}
