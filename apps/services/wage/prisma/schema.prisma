datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_URL")
    directUrl = env("POSTGRES_DIRECT_URL")
}

generator client {
    provider = "prisma-client-js"
    output   = "./client/wage"
}

model Statement {
    taskID    String   @id @db.Uuid
    profID    String   @db.VarChar(256)
    wage      Decimal  @db.Decimal(15, 2)
    createdAt DateTime @default(now()) @db.Timestamp(0)
}
